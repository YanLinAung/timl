(defmacro include-guard (...)
  (let ((param (or (first ...) 'b:did-ftplugin)))
   `(timl#core#include-guard ,param)))

; should append to b:undo-ftplugin
(defmacro setlocal (...)
  `(do
     (:setlocal ,(apply build-option-string ...))
     (setq b:undo-ftplugin ,(apply extract-option-restore ...))))

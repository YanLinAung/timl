(defmacro include-guard [& default]
  (let [param (or (first more) 'b:did-ftplugin)]
   `(timl#core#include-guard ~param)))

; should append to b:undo-ftplugin
(defmacro setlocal [& opts]
  `(do
     (: (str "setlocal " ~(build-option-string opts)))
     (set! b:undo-ftplugin ~(extract-option-restore opts))))

(in-ns 'timl#core)

(defn conj [coll & xs] (append coll xs))

(defn sorter [>]
  (fn [x y]
    (if (> x y)
      1
      (if (> y x)
        -1
        0))))

(defn sort [xs & args]
  (if args
    (f:sort (f:copy (first args)) (sorter xs) {})
    (f:sort (f:copy xs))))

(defn get-in [m ks] (reduce get m ks))
